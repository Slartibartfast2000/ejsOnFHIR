<table id="searchResults">
  <thead>
    <tr>
      <th>ID</th>
      <th>Family Name</th>
      <th>Given Name</th>
      <th>Gender</th>
      <th>Birth Date</th>
    </tr>
  </thead>
  <tbody>
    <% entry.forEach(patient => { %>
      <tr data-id="<%= patient.resource.id %>">
        <td><%= patient.resource.id %></td>
        <td><%= patient.resource.name[0].family %></td>
        <td><%= patient.resource.name[0].given[0] %></td>
        <td><%= patient.resource.gender %></td>
        <td><%= patient.resource.birthDate %></td>
        <td>
          <button class="delete-btn" data-id="<%= patient.resource.id %>">Delete</button>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>

